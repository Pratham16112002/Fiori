{
  "_version": "1.65.0",
  "sap.app": {
    "id": "flightconn",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.2",
      "toolsId": "7d877ba0-2f32-46df-b49a-4f6033783cf3"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZPP_CONN_FL_SD/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.133.4",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "flightconn.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "flightconn.view",
        "async": true,
        "viewPath": "flightconn.view"
      },
      "routes": [
        {
          "name": "RouteFlightConnection",
          "pattern": ":?query:",
          "target": [
            "TargetFlightConnection"
          ]
        },
        {
          "name": "CarrierDetail",
          "pattern": "carrier/{carrierId}/{connId}",
          "target": [
            "CarrierDetail"
          ]
        }
      ],
      "targets": {
        "TargetFlightConnection": {
          "id": "FlightConnection",
          "name": "FlightConnection"
        },
        "CarrierDetail": {
          "id": "CarrierDetail",
          "name": "carrier/CarrierDetail",
          "level": 2,
          "transition": "slide"
        }
      }
    },
    "rootView": {
      "viewName": "flightconn.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
