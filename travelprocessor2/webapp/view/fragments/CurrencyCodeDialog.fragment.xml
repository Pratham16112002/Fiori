<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		id="CurrencyCodeSelectDialog"
		noDataText="No Currency Found"
		title="Select Product"
		search=".onSearch"
		confirm=".onConfirmDialogClose"
		cancel=".onCancelDialogClose"
		items="{
			path: '/I_Currency'
		}">
		<StandardListItem
			title="{Currency}"
			description="{Currency_Text}"
			 />
	</SelectDialog>
</core:FragmentDefinition>