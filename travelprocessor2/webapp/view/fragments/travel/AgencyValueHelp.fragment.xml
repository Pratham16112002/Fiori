<core:FragmentDefinition xmlns:valuehelpdialog="sap.ui.comp.valuehelpdialog"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:t="sap.ui.table"
    xmlns:fb="sap.ui.comp.filterbar"
>
    <valuehelpdialog:ValueHelpDialog
		title="Product"
		ok=".onOk"
		cancel=".onCancel"
		afterClose=".onAfterClose"
		supportRanges="true"
		supportMultiselect="true"
		key="AgencyID"
		descriptionKey="Name">
		<valuehelpdialog:filterBar>
			<fb:FilterBar advancedMode="true" search=".onSearch" isRunningInValueHelpDialog="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="AgencyID"
						label="Agency ID"
						visibleInFilterBar="true">
						<fb:control>
							<Input name="AgencyID"></Input>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="Name"
						label="Name"
						visibleInFilterBar="true">
						<fb:control>
							<Input name="Name" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</valuehelpdialog:filterBar>
	</valuehelpdialog:ValueHelpDialog>
</core:FragmentDefinition>
