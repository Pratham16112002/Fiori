<core:FragmentDefinition
    xmlns:valuehelpdialog="sap.ui.comp.valuehelpdialog"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:t="sap.ui.table"
    xmlns:fb="sap.ui.comp.filterbar"
>
    xmlns:valuehelpdialog="sap.ui.comp.valuehelpdialog"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:t="sap.ui.table"
    <valuehelpdialog:ValueHelpDialog
        title="Agency"
        ok=".onOk"
        cancel=".onCancel"
        afterClose=".onAfterClose"
        supportRanges="false"
        supportMultiselect="false"
        key="AgencyID"
        descriptionKey="Name"
    >
        <valuehelpdialog:filterBar>
            <fb:FilterBar
                advancedMode="true"
                search=".onSearch"
                isRunningInValueHelpDialog="true"
            >
                <fb:filterGroupItems>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="AgencyID"
                        label="Agency ID"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <Input name="AgencyID" />
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem
                        groupName="__$INTERNAL$"
                        name="Name"
                        label="Name"
                        visibleInFilterBar="true"
                    >
                        <fb:control>
                            <Input name="Name" />
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </valuehelpdialog:filterBar>
    </valuehelpdialog:ValueHelpDialog>
</core:FragmentDefinition>
